                                <apex:actionFunction action="{!InsertarArea}" name="InsertarArea" rerender="cadaArea" oncomplete="resetCamposHito()" />
                            </div>
                        </div>
                    </div>
                </div>
                
                <!-- modal - Nuevo contacto de reto -->

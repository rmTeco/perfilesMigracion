                                                                                        </apex:actionStatus>
                                                                                    </div>
                                                                                    
                                                                                    
                                                                                </td>
                                                                            </tr>
                                                                        </apex:repeat>
                                                                    </tbody>
                                                                </table>
                                                                
                                                                
                                                                <div>
                                                                    <apex:actionStatus id="agregandoContacto">
                                                                        <apex:facet name="start">
                                                                            <img src="/img/loading.gif"/>
                                                                        </apex:facet>
                                                                    </apex:actionStatus>

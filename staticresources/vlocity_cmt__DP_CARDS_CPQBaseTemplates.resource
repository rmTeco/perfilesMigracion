                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/* $cpq-namespace is defined in cpq-base-variables */\n$namespace: $cpq-namespace;\n\n/*** STYLES ***/\n.vlocity {\n    &.via-slds {\n        .#{$namespace}-product-item-details {\n            .#{$namespace}-product-item-details-gallery {\n                margin-top: $cpq-padding-large-vertical*2;\n                ul {\n                    height: 140px;\n                    overflow-x: auto;\n                    white-space: nowrap;\n                    padding: $cpq-padding-large-vertical 0px;\n                    li {\n                        width: 120px;\n                        height: 100px;\n                        padding: $cpq-padding-small-horizontal;\n                        margin-right: $cpq-padding-small-horizontal;\n                        display: inline-block;\n                        border: 1px solid $middle-light-gray;\n                        vertical-align: middle;\n                        text-align: center;\n                        &:hover{\n                            img {\n                                transform: scale(1.5,1.5);\n                                transition: .3s transform;\n                            }\n                        }\n                        img{\n                            max-width: 100%;\n                            height: auto;\n                            transform: translateZ(0);\n                        }\n                    }\n                }\n            }\n            .#{$namespace}-product-item-details-features {\n                margin-top: $cpq-padding-large-vertical*2;\n            }\n            .#{$namespace}-product-item-details-form {\n                max-width: 400px;\n            }\n            .#{$namespace}-product-item-details-addon {\n                position: relative; //needed for selected border\n                
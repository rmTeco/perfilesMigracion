t-item-table-row {\n              display: flex;\n              flex-flow: row nowrap;\n              flex: 1 1 auto;\n            }\n            .#{$namespace}-product-item-table-row-item {\n              display: flex;\n              flex-flow: row nowrap;\n              flex: 1 0 auto;\n              word-wrap: break-word;\n              overflow-wrap: break-word;\n                .#{$namespace}-price-label {\n                  font-size: $cpq-cart-item-attr-label-font-size;\n                }\n                .#{$namespace}-loyalty-points {\n                    color: $cpq-loyalty-points-color;\n                }\n            }\n            .#{$namespace}-product-item-flex-icon {\n                flex: 0 0 25px;\n            }\n            .#{$namespace}-product-item-flex-product {\n              flex: 1 1 300px;\n            }\n            .#{$namespace}-product-item-flex-childProduct {\n              flex: 1 1 255px;\n            }\n            .#{$namespace}-product-item-flex-grandChildProduct {\n              flex: 1 1 230px;\n            }\n            .#{$namespace}-product-item-flex-recur {\n                flex: 1 0 150px;\n                justify-content: flex-end;\n            }\n            .#{$namespace}-product-item-flex-onetime {\n                flex: 1 0 150px;\n                justify-content: flex-end;\n            }\n        }\n    }\n}",

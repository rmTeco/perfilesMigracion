de-button-icon {\n                            position: absolute;\n                            top: 50%;\n                            left: 50%;\n                            transform: translateY(-50%) translateX(-50%);\n                            fill: #fff;\n                            width: 1.25rem;\n                            height: 1.25rem;\n                            transition: transform 500ms ease-in;\n                        } \n                    }\n                }\n\n                &.#{$namespace}-left-sidebar-visible {\n                    margin-left: 0;\n                    left: 0;\n                    box-shadow: 1px 0 5px rgba(0,0,0,0.3);\n\n                    .#{$namespace}-left-sidebar-container {\n                        button.#{$namespace}-show-hide-button {\n                            > .slds-button__icon.#{$namespace}-show-hide-button-icon {\n                                transform: translateY(-50%) translateX(-50%) rotate(180deg);\n                            }\n                        }\n                    }\n                }\n            }\n\n            .#{$namespace}-cart-container-wrapper {\n\n            }\n\n            .#{$namespace}-product-container-wrapper {\n                transition-property: right;\n                position: fixed;\n                z-index: 999;\n                margin-right: 0;\n                right: -33.33%;\n                top: 0;\n                height: 100%;\n                overflow-y: auto;\n                background-color: #fff;\n\n                ng-include {\n                    display: block;\n                }\n\n                &.#{$namespace}-product-container-visible {\n                    margin-right: 0;\n                    right: 0;\n                    box-shadow: 0 1px 5px rgba(0,0,0,0.3);\n                }\n            }\n        }\n\n        // 48rem\n        @media screen and (max-width: $bp-medium) {\n\n        }\n\n        // 30rem\n        @media screen and (max-width: $bp-small) {\n\n        }\n    }\n}\n\n[dir=rtl] .vlocity {\n    &.via-slds {\n        .#{$namespace}-left-sidebar-wrapper {\n            transition-property: margin-right;\n            margin-right: -33.33%;\n            margin-left: 0;\n\n            &.#{$namespace}-left-sidebar-visible {\n                margin-right: 0;\n            }\n        }\n        .#{$namespace}-product-container-wrapper {\n            transition-property: margin-left;\n            margin-left: -33.33%;\n            margin-right: 0;\n\n            &.#{$namespace}-product-container-visible {\n                margin-left: 0;\n            }\n        }\n        .#{$namespace}-show-hide-button {\n            margin-left: 0;\n            position: relative;\n            left: 8px;\n            margin-right:0;\n        }\n\n        // 64rem\n        @media screen and (max-width: $bp-large) {\n            .#{$namespace}-left-sidebar-wrapper {\n                transition-property: right;\n                right: -33.33%;\n                left: auto;\n                margin-right: 0;\n\n                .#{$namespace}-left-sidebar-container {\n                    button.#{$namespace}-show-hide-button {\n                        left: -2.75rem;\n                        right: auto;\n                        border-radius: 0.25rem 0 0 0.25rem;\n\n                        > .slds-button__icon.#{$namespace}-show-hide-button-icon {\n                            transform: translateY(-50%) translateX(-50%) rotate(180deg);\n                        }\n                    }\n                }\n\n                &.#{$namespace}-left-sidebar-visible {\n                    right: 0;\n                    left: auto;\n\n                    .#{$namespace}-left-sidebar-container {\n                        button.#{$namespace}-show-hide-button {\n                            > .slds-button__icon.#{$namespace}-show-hide-button-icon {\n                                transform: translateY(-50%) translateX(-50%);\n   
                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/* $cpq-namespace is defined in cpq-base-variables */\n$namespace: $cpq-namespace;\n\n/*** STYLES ***/\n.vlocity {\n    &.via-slds {\n        .#{$namespace}-left-sidebar {\n            position: absolute;\n            top: 0;\n            right: $cpq-padding-large-horizontal/2;\n            bottom: 0;\n            left: $cpq-padding-large-horizontal/2;\n\n            .#{$namespace}-searchbox {\n                vertical-align: middle;\n                margin-bottom: $cpq-padding-large-vertical;\n                \n                input {\n                   background-color: $cpq-product-container-bg-color;\n                   border-radius: 0px;\n                   border: 1px solid $cpq-product-container-bg-color;;\n                }\n            }\n            .#{$namespace}-tabs-container { \n                padding-bottom: 10px;\n            }\n            .#{$namespace}-filter-container {\n                background-color: $cpq-product-container-bg-color;\n                margin-bottom: $cpq-product-item-padding;\n                section {\n                    padding: $cpq-padding-base-vertical 0px;\n                    margin: 0px $cpq-padding-large-horizontal;\n                }\n                \n                a {\n                    text-decoration: none;\n                    color: $mid-black;\n                    \n                    &:hover {\n                        color:$dark-blue;\n                    }\n                }\n            }\n            .#{$namespace}-items-container {\n                overflow-y: scroll;\n                -webkit-overflow-scrolling: touch;\n                background-color: $cpq-product-container-bg-color;\n\n                //HYB-2028\n                display:flex;\n\n                *[tabview],\n                ng-include[hotkeys-layout-navigation]{\n                    flex:1;\n                    display:flex;\n                }\n            }\n        }\n\n        // 64rem\n        @media screen and (max-width: $bp-large) {\n            .#{$namespace}-left-sidebar {\n                top: $cpq-padding-large-vertical*1.2;\n                right: $cpq-padding-large-horizontal*0.5;\n                bottom: $cpq-padding-large-vertical*1.2;\n                left: $cpq-padding-large-horizontal*0.5;\n            }\n        }\n    }\n}",

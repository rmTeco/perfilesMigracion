                        "%vlocity_namespace%__HTML__c": "<div class=\"cpq-cart-item-product-children slds-col scroll\">\n    <!--search items code block -->\n    <div ng-controller=\"CPQCartItemController\">\n        <div ng-controller=\"searchItemsTplController\" ng-if=\"records.actions.getproducts\">\n            <div class=\"slds-form-element slds-lookup slds-is-open\" data-select=\"single\">\n                <label class=\"slds-form-element__label\" for=\"lookup-366\">Search Item</label>\n                <div class=\"slds-form-element__control\">\n                    <div class=\"slds-input-has-icon slds-input-has-icon_left\">\n                        <slds-svg-icon sprite=\"'utility'\" size=\"'x-small'\" icon=\"'search'\" extra-classes=\"'slds-input__icon slds-icon-text-default'\"></slds-svg-icon>\n                        <input class=\"slds-lookup__search-input slds-input\" type=\"search\" placeholder=\"Search\" role=\"combobox\" aria-activedescendent=\"\" aria-expanded=\"true\" aria-autocomplete=\"list\" ng-model=\"searchItem\" slds-options=\"item.UnitPrice.value  as item.Name.value for item in getSearchItems(records, $viewValue, tempScope)\" data-template=\"{{typeaheadTemplate}}\" data-trigger=\"focus\" data-limit=\"100\" slds-typeahead/>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <vloc-card ng-repeat=\"card in cards | orderBy: 'order'\" \n    \tctrl=\"CPQCartItemController\" \n    \tng-model=\"card\" data=\"card\" records=\"records\" index=\"{{$index}}\"\n    \taria-level=\"{{(attrs.ariaLevel - 0) + 1}}\" \n    \tline-item-modal=\"{{attrs.lineItemModal}}\" \n    \tprocessing-line=\"{{attrs.processingLine}}\"\n    \tprovisioning-status=\"{{attrs.provisioningStatus}}\"\n        show-product-list=\"{{attrs.showProductList}}\"\n        show-config-panel=\"{{attrs.showConfigPanel}}\"\n        class=\"cpq-item-product-child-level-{{(attrs.ariaLevel - 0) + 1}}\">\n    </vloc-card>\n</div>",

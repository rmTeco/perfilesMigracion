lds-text-body_small\"> \n                            {{::importedScope.getPriceValue(obj, 'UnitPrice')}}\n                        </span>\n                        <span ng-if=\"::obj[$root.nsPrefix + 'OneTimeLoyaltyPrice__c']\">\n                            <span class=\"slds-text-body_small\">\n                                &nbsp;{{::importedScope.customLabels.CPQOr}}\n                            </span>\n                            <span class=\"slds-text-body_small cpq-loyalty-points\">\n                                &nbsp;{{::obj[$root.nsPrefix + 'OneTimeLoyaltyPrice__c'].value}} \n                            </span>\n                            <span class=\"slds-text-body_small cpq-loyalty-points\">\n                                {{::obj.LoyaltyCode}}\n                            </span>\n                        </span>\n                        <div class=\"cpq-item-product-messages\">\n                            <ul ng-if=\"obj.messages.length > 0\">\n                                <li ng-repeat=\"msg in obj.messages\" class=\"slds-p-vertical_xx-small\">\n                                    <span class=\"cpq-error-msg\" ng-if=\"msg.severity === 'ERROR'\">\n                                        {{msg.message}}\n                                    </span>\n                                    <span class=\"cpq-warning-msg\" ng-if=\"msg.severity !== 'ERROR'\">\n                                        {{msg.message}}\n                                    </span>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                    <div class=\"slds-col slds-small-size_4-of-12 slds-medium-size_5-of-12 slds-large-size_4-of-12 slds-text-align_right cpq-product-actions\">\n                        <button class=\"slds-button slds-button_neutral cpq-add-button\" ng-click=\"importedScope.addToCart(obj)\" ng-disabled=\"!obj.actions || !obj.actions.addtocart || obj.category.toLowe
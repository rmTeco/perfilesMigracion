                        "%vlocity_namespace%__Sass__c": "/*** ASSETS THEME VARIABLES ***/\n@import \"comp-assets-theme-variables\";\n\n/* $assets-namespace is defined in cpq-base-variables */\n$namespace: $assets-namespace;\n\n/*** STYLES ***/\n.vlocity {\n    &.via-slds {\n        .comp-#{$namespace}-promotions {\n            .#{$namespace}-promo-items-tab-content {\n                padding-top: 0px; /* slds override */\n            }\n            .#{$namespace}-promo-item-border {\n                padding: 10px 0px;\n                margin: 0px 16px;\n                border-bottom: 1px solid #d8dde6;\n            }\n            .#{$namespace}-base-promo {\n                display: flex;\n                flex-flow: row nowrap;\n                justify-content: flex-end;\n\n                &.#{$namespace}-promo-header {\n                    font-size: 11px;\n                    @extend .#{$namespace}-promo-item-border;\n                }\n                &.#{$namespace}-promo-item {\n                    @extend .#{$namespace}-promo-item-border;\n                }\n                .#{$namespace}-promo-name {\n                    flex: 5;\n                    padding: 0 6px 0 0;\n\n                    .#{$namespace}-promo-item-bookmark-change, .#{$namespace}-promo-item-bookmark-existing, .#{$namespace}-promo-item-bookmark-disconnect {\n                        color: $product-item-bookmark-red;\n                    }\n                    .#{$namespace}-promo-item-bookmark-add, .#{$namespace}-promo-item-bookmark-default {\n                        color: $product-item-bookmark-blue;\n                    }\n                }\n            }\n            .#{$namespace}-no-items-msg {\n                padding: 40px 10px;;\n            }\n       }\n    }\n}",

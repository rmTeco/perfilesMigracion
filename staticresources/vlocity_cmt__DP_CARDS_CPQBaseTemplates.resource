                        "%vlocity_namespace%__Sass__c": ".vlocity {\n    /* Uncomment for Vlocity Editor preview */\n    min-height: 640px;\n    \n    .home-container {\n        $card-padding-top: 2em;\n        $card-padding-horizontal: 2em;\n        $card-padding-bottom: 2em;\n        $card-padding: $card-padding-top\n                       $card-padding-horizontal\n                       $card-padding-bottom;\n        \n        position: absolute;\n        top: 0;\n        bottom: 0;\n        left: 0;\n        right: 0;\n        margin: 0;\n        padding: 0;\n        \n        font-size: 1rem;\n        \n        background-size: cover;\n        \n        .column-divider {\n            position: absolute;\n            top: $card-padding-top + 0.5em;\n            bottom: $card-padding-bottom + 0.5em;\n            left: 50%;\n            width: 1px;\n            \n            background-color: #b7d4f3;\n        }\n        \n        .left-coloumn {\n            position: absolute;\n            top: 0;\n            left: 0;\n            right: 50%;\n            bottom: 0;\n            \n            margin: 0;\n            padding: 0;\n        }\n        \n        \n        .right-coloumn {\n            position: absolute;\n            top: 0;\n            bottom: 0;\n            left: 50%;\n            right: 0;\n            \n            margin: 0;\n            padding: 0;\n        }\n        \n        .left-wrapper, .right-wrapper {\n            position: absolute;\n            top: 0;\n            bottom: 0;\n            left: 0;\n            right: 0;\n            margin: $card-padding;\n            padding: 0;\n            \n            .home-actions {\n                position: absolute;\n                top: 0;\n                bottom: 0;\n                left: 0;\n                right: 0;\n            }\n        }\n        \n        .cards-container{\n            overflow: visible;\n        }\n            \n        .left-wrapper {\n            background-color: #159DE2;\n            border-radius: 5px;\n            box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.2);\n        }\n        \n        .home-logo {\n        \n            background-image: url(https://c.na35.content.force.com/servlet/servlet.ImageServer?id=01541000001fws2&oid=00D41000002IGRk&lastMod=1487366364000);\n            background-size: contain;\n            background-repeat: no-repeat;\n            background-position: center top;\n            \n            position: absolute;\n            top: 14%;\n            left: 15%;\n            right: 15%;\n            height: 20%;\n         }\n         \n        .left-content {\n            position: absolute;\n            top: 35%;\n            bottom: 5em;\n            left: 1em;\n            right: 1em;\n        }\n         \n        .search-box {\n            input {\n                background-color: #0286C5;\n                border: none;\n                font-size: 0.8em;\n                font-weight: bold;\n                color: #FFF;\n                padding: 0.5rem 1rem;\n                 \n                &::placeholder {\n                    color: #FFF;\n                    font-weight: bold;\n                }\n                \n                &::-webkit-input-placeholder {\n                    color: #FFF;\n                    font-weight: bold;\n                }\n            }\n        }\n         \n        .welcome-message {\n            text-align: center;\n            color: #FFF;\n            margin-top: 2em;\n            padding: 0 1.5em;\n        }\n        \n        .learn-more-link {\n            margin-top: 1em;\n            text-align: center;\n            \n            a {\n                padding-bottom: 0.25em;\n                border-bottom: 2px solid #FFF;\n                color: #FFF;\n                \n                &:link, &:active, &:hover, &:visited {\n                    color: #FFF;\n                    text-decoration: none;\n                }\n            }\n        }\n        \n        .search-results {\n            position: absolute;\n            top: 2em;\n            left: 0;\n            right: 0;\n            bottom: 0;\n            margin-top: 0.25em;\n            background: white;\n            overflow-y: auto;\n            \n            border-radius: 5px;\n            box-shadow: 0 2px 2px 2px rgba(0, 0, 0, 0.2);\n                \n            li {\n                position: relative;\n                border-bottom: 1px solid #E0E5EE;\n                padding: 1em;\n            }\n            \n            .search-result-name {\n                color: #061C3F;\n            }\n            \n            .search-result-phone {\n                color: #4B749A;\n            }\n            \n            .search-result-location {\n                position: absolute;\n                top: 1em;\n                right: 1em;\n                \n                color: #4B749A;\n            }\n        }\n         \n        .sign-out-btn {\n            position: absolute;\n            bottom: 1.8em;\n            width: 100%;\n            text-align: center;\n            \n            button {\n                color: #FFF;\n                font-size: 1em;\n            }\n        }\n   }\n}",

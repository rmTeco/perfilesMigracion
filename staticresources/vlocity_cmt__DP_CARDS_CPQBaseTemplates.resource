                        "%vlocity_namespace%__HTML__c": "<div class=\"slds-grid slds-grid_vertical comp-cart-review\" ng-controller=\"AssetsMainController\">\n\t<!--The scroll Class is used for mobile devices -->\n\t<div class=\"slds-col comp-cart-review-items scroll\" >\n\t    <vloc-card ng-repeat=\"card in cards\" ctrl=\"AssetsProductItemController\" \n            ng-model=\"card\" data=\"card\" records=\"records\" \n            on-loaded=\"$first ? getCardsCallback() : null\"\n            cart-id=\"{{attrs.cartId}}\" \n            pricing-mode=\"{{attrs.pricingMode}}\" \n            promotion-mode=\"{{attrs.promotionMode}}\" \n            show-config-panel=\"{{attrs.showConfigPanel}}\" \n            index=\"{{$index}}\" session-id=\"{{sessionId}}\">\n\t    </vloc-card>\n\t</div>\n\n    <div class=\"slds-grid slds-grid_vertical-align-center slds-grid_align-center\" ng-if=\"!records || records.length === 0 \">\n      {{::importedScope.customLabels.CPQCartIsEmpty}}\n    </div>\n\n    <div class=\"next-product slds-col slds-text-align_center slds-m-vertical_medium\"  ng-if=\"session.nextProducts\">\n        <div class=\"slds-spinner_container\" ng-show=\"loadingMore\">\n            <div class=\"slds-spinner_brand slds-spinner slds-spinner_small\" aria-hidden=\"false\" role=\"alert\">\n                <div class=\"slds-spinner__dot-a\"></div>\n                <div class=\"slds-spinner__dot-b\"></div>\n            </div>\n        </div>\n        <a ng-click=\"importedScope.nextPageProducts()\" ng-hide=\"loadingMore || !isLoaded\">\n          {{::importedScope.customLabels.CPQLoadMore}}\n        </a>\n    </div>\n\n</div>\n",

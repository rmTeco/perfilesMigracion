                        "%vlocity_namespace%__HTML__c": "<div class=\"cpq-base-grid\">\n\n    <div class=\"slds-spinner_container\" ng-show=\"!isLoaded\">\n        <div class=\"slds-spinner_brand slds-spinner slds-spinner_medium\" aria-hidden=\"false\" role=\"alert\">\n            <div class=\"slds-spinner__dot-a\"></div>\n            <div class=\"slds-spinner__dot-b\"></div>\n        </div>\n    </div>\n\n    <div class=\"slds-grid slds-grid_frame slds-grid_vertical\">\n        <vloc-layout layout-name=\"cpq-header\" enable-pricing=\"{{importedScope.featureSettings.enablePricing}}\"  enable-loyalty-points=\"{{importedScope.customSettings.EnableLoyaltyPoints}}\"></vloc-layout>\n\n        <div class=\"slds-col slds-grid slds-wrap slds-small-nowrap cpq-container\" ng-class=\"{'cpq-outer-col-visible': !importedScope.showProductList || importedScope.isConfigAttrsPanelEnabled}\">\n            <div class=\"slds-backdrop cpq-tablet-overlay\" ng-if=\"importedScope.isMobileTablet\" ng-click=\"importedScope.toggleOutsideCols()\" ng-class=\"{'slds-backdrop_open': !importedScope.showProductList || importedScope.isConfigAttrsPanelEnabled}\"></div>\n\n            <div class=\"slds-is-relative slds-col slds-grid slds-size_1-of-1 slds-small-size_1-of-3 slds-medium-size_2-of-6 slds-large-size_4-of-12 cpq-left-sidebar-wrapper\" ng-class=\"{'cpq-left-sidebar-visible': !importedScope.showProductList && !importedScope.isConfigAttrsPanelEnabled}\">\n               \n                <div class=\"cpq-left-sidebar-container\">\n                    <button ng-if=\"importedScope.isMobileTablet\" class=\"slds-button slds-button_icon-border cpq-show-hide-button\" ng-click=\"importedScope.toggleOutsideCols()\">\n                        <slds-button-svg-icon sprite=\"'utility'\" size=\"'medium'\" icon=\"'right'\" extra-classes=\"'cpq-show-hide-button-icon'\"></slds-button-svg-icon>\n                    </button>\n                    <vloc-layout layout-name=\"cpq-left-sidebar\" ctrl=\"CPQItemsController\"></vloc-layout>\n                </div>\n\n            </div>\n\n            <div class=\"slds-is-relative slds-col slds-grid slds-size_1-of-1 cpq-cart-container-wrapper\" ng-class=\"{'slds-small-size_2-of-3 slds-medium-size_4-of-6 slds-large-size_8-of-12': importedScope.isConfigAttrsPanelEnabled || (!importedScope.showProductList && !importedScope.isConfigAttrsPanelEnabled), 'slds-small-size_3-of-3 slds-medium-size_6-of-6 slds-large-size_12-of-12': importedScope.showProductList && !importedScope.isConfigAttrsPanelEnabled}\">\n\n                <!-- Hide/Show Product List -->\n                <button ng-if=\"!importedScope.isMobileTablet\" class=\"slds-button slds-button_icon-border cpq-show-hide-button\" ng-class=\"{'cursor-point-west': !importedScope.showProductList, 'cursor-point-east': importedScope.showProductList}\" ng-click=\"importedScope.toggleOutsideCols()\">\n                </button>\n\n                <div class=\"cpq-cart-container\">\n                    <vloc-layout layout-name=\"cpq-product-cart\" ctrl=\"CPQCartController\" show-product-list=\"{{importedScope.showProductList}}\" show-config-panel=\"{{importedScope.isConfigAttrsPanelEnabled}}\"></vloc-layout>\n                </div>\n\n                <div>\n                    <vloc-layout layout-name=\"cpq-total-bar\" ctrl=\"CPQTotalController\"></vloc-layout>\n                </div>\n\n            </div>\n            \n            <div class=\"slds-is-relative slds-col slds-grid slds-size_1-of-1 slds-small-size_1-of-3 slds-medium-size_2-of-6 slds-large-size_4-of-12 cpq-product-container-wrapper\" ng-class=\"{'cpq-product-container-visible': importedScope.isConfigAttrsPanelEnabled}\">\n\n                <div class=\"cpq-product-container\">\n                    <vloc-layout layout-name=\"cpq-product-cart-config\" ctrl=\"CPQCartItemConfigController\"></vloc-layout>\n                </div>\n\n            </div>\n\n        </div>\n\n    </div>\n\n</div>",

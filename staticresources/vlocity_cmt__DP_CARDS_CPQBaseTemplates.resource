                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/* $cpq-namespace is defined in cpq-base-variables */\n$namespace: $cpq-namespace;\n\n/*** STYLES ***/\n.vlocity {\n    &.via-slds {\n        .#{$namespace}-promotion-item {\n            border-bottom: $cpq-product-item-padding solid $cpq-page-bg-color;\n            \n            .#{$namespace}-promotion-item-container {\n                padding: $cpq-padding-large-vertical 0px;\n                margin: 0px $cpq-padding-large-horizontal;\n            \n                .#{$namespace}-product-item-select-checkbox {\n                    // Ensure atleast 40px width for inputs for device touch area\n                    padding: $cpq-padding-small-horizontal;\n                    margin-top: -$cpq-padding-small-horizontal/2;\n                }\n                \n                .#{$namespace}-promotion-name {\n                    font-weight: $cpq-headings-font-weight;\n                    line-height: 30px;\n                }\n                \n                .#{$namespace}-promotion-info div {\n                    padding-right: $cpq-padding-large-horizontal;\n                    padding-top:$cpq-padding-large-vertical;\n                    color: $dark-gray;\n                    &:last-child {\n                        padding-bottom:$cpq-padding-large-vertical;\n                    }\n                    \n                    .#{$namespace}-promotion-label {\n                      font-size: $cpq-cart-item-attr-label-font-size;\n                    }\n                }\n\n                .#{$namespace}-promotion-info .#{$namespace}-item-promotion-messages {\n                    font-size: 95%;\n\n                    .#{$namespace}-error-msg {\n                        color: $error;\n                    }\n                    .#{$namespace}-warning-msg {\n                        color: $warning;\n                    }\n                }\n            }\n            \n            .#{$namespace}-promotion-actions {\n                padding-top:$cpq-padding-large-vertical;\n                padding-bottom:$cpq-padding-large-vertical; \n                \n                .#{$namespace}-add-button {\n                    padding-left:$cpq-padding-small-horizontal;\n                    padding-right:$cpq-padding-small-horizontal;\n               }\n            }\n        }\n    }\n}",

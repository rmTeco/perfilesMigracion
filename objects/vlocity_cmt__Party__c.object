IF( vlocity_cmt__PartyEntityType__c ==&apos;Account&apos;, vlocity_cmt__AccountId__r.BillingStreet &amp; BR() &amp; 
IF( ISBLANK(vlocity_cmt__AccountId__r.BillingCity), &quot;&quot;, vlocity_cmt__AccountId__r.BillingCity) &amp; IF(AND(NOT(ISBLANK(vlocity_cmt__AccountId__r.BillingCity)), OR( NOT(ISBLANK(vlocity_cmt__AccountId__r.BillingState)), NOT(ISBLANK(vlocity_cmt__AccountId__r.BillingPostalCode)), NOT(ISBLANK(vlocity_cmt__AccountId__r.BillingCountry)))), &quot;, &quot;, &quot; &quot;) &amp; vlocity_cmt__AccountId__r.BillingState &amp; &quot; &quot; &amp; vlocity_cmt__AccountId__r.BillingPostalCode &amp; BR() &amp; 

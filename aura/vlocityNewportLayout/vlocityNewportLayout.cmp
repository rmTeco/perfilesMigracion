<aura:component implements="forceCommunity:layout" description="Vlocity NewPort layout" access="global">
    <aura:attribute name="header" description="Content Header" type="Aura.Component[]" required="false"></aura:attribute>
    <aura:attribute name="content" type="Aura.Component[]" required="false"></aura:attribute>
    <aura:attribute name="sidebar" type="Aura.Component[]" required="false"></aura:attribute>
    <aura:attribute name="footer" description="Content Footer" type="Aura.Component[]" required="false"></aura:attribute>
    
    <div class="slds-col--padded">
        {!v.header}
    </div>
    <div class="slds-grid slds-wrap" >
        <div class="slds-col--padded slds-size--12-of-12 slds-medium-size--8-of-12 slds-large-size--8-of-12 column1" >
            {!v.content}
        </div>
        <div class="slds-col--padded slds-size--12-of-12 slds-medium-size--4-of-12 slds-large-size--4-of-12 column2" >
            {!v.sidebar}
        </div>
    </div>
     <div class="slds-col--padded">
        {!v.footer}
    </div>
</aura:component>
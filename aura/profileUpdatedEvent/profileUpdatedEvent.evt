<aura:event access="GLOBAL" type="APPLICATION" description="Fired when a change has occured to the profile">
    <aura:attribute name="attributeCode" type="String" description="Attribute Code that was updated" />
    <aura:attribute name="attributeCategoryCode" type="String" description="Attribute Category Code that was updated" />
    <aura:attribute name="externalUpdate" type="Boolean" description="Did this update come from outside the current profiler instance, e.g. PushTopic" />
</aura:event>